{
  "name": "cdk-docgen",
  "version": "0.1.0",
  "description": "Generates docs for AWS CDK jsii modules",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CDK-User-Group/cdk-docgen.git"
  },
  "bin": {
    "cdk-gendoc": "bin/cdk-gendoc"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest"
  },
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "dependencies": {
    "fs-extra": "^8.1.0",
    "glob-promise": "^3.4.0",
    "glob": "^7.1.6",
    "jsii-reflect": "^0.21.1",
    "jsii-spec": "^0.20.11",
    "showdown-highlight": "^2.1.3",
    "yaml": "^1.7.2",
    "yargs": "^15.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "statements": 80
      }
    }
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/yaml": "^1.2.0",
    "@types/yargs": "^16.0.1",
    "jest": "^24.9.0",
    "jsii": "^0.21.1",
    "typescript": "^3.7.4"
  },
  "keywords": [
    "cdk"
  ],
  "homepage": "https://github.com/CDK-User-Group/cdk-docgen",
  "engines": {
    "node": ">= 8.10.0"
  }
}
